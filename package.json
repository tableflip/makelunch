{
  "name": "makelunch-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "find ./test -name '*.js' -exec node {} \\;",
    "start": "node server.js",
    "build": "npm-run-all --parallel build:*",
    "watch": "npm-run-all --parallel watch:*",
    "minify": "npm-run-all --parallel minify:*",
    "build:js": "browserify -t jadeify ./client.js -o public/bundle.js",
    "build:css": "postcss -c postcss.config.js -o public/bundle.css",
    "watch:js": "watchify -t jadeify ./client.js -o public/bundle.js",
    "watch:css": "postcss -c postcss.config.js -o public/bundle.css -w",
    "watch:server": "nodemon --ignore node_modules/ --ignore public/ -e jade,js,json server.js",
    "minify:js": "uglifyjs public/bundle.js -o public/bundle.js",
    "minify:css": "postcss -u cssnano -i public/bundle.css -o public/bundle.css",
    "hashify": "cd public; rm bundle.*.*; hashmark -l 8 bundle.{css,js} 'bundle.{hash}{ext}' --asset-map manifest.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "basscss": "^7.1.1",
    "basscss-btn-sizes": "^1.1.1",
    "basscss-responsive-white-space": "^0.1.0",
    "browserify": "^13.0.0",
    "config": "^1.19.0",
    "cssnano": "^3.4.0",
    "ecstatic": "^1.4.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "express-session": "^1.13.0",
    "jade": "^1.11.0",
    "jadeify": "^4.6.0",
    "jquery": "^2.2.0",
    "jsonwebtoken": "^5.7.0",
    "normalize.css": "^3.0.3",
    "npm-run-all": "^1.5.1",
    "page": "^1.6.4",
    "passport": "^0.3.2",
    "passport-twitter": "^1.0.4",
    "postcss": "^5.0.14",
    "postcss-calc": "^5.2.0",
    "postcss-cli": "^2.4.0",
    "postcss-color-function": "^2.0.0",
    "postcss-custom-media": "^5.0.0",
    "postcss-custom-properties": "^5.0.0",
    "postcss-discard-comments": "^2.0.3",
    "postcss-import": "^8.0.2",
    "require-dir": "^0.3.0",
    "rethinkdb": "^2.2.1",
    "rethinkdb-init": "0.0.5",
    "shortid": "^2.2.4"
  },
  "devDependencies": {
    "quickreload": "^1.1.0",
    "standard": "^5.4.1",
    "tape": "^4.4.0",
    "watchify": "^3.7.0"
  }
}
